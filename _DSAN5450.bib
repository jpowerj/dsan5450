  keywords = {computational sociology,content analysis,culture,methodology,text analysis,word embeddings,word2vec},
  file = {/Users/jpj/Zotero/storage/4PNS6E8S/Kozlowski et al. - 2019 - The Geometry of Culture Analyzing the Meanings of.pdf}
}

